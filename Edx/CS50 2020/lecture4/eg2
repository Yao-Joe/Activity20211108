/*
eg2 verify that we can get continuous memory address even if the string is in the end.
*/

# include <stdio.h>
# include <cs50.h>

int main(void)
{
    string s = "HI!";
    printf("%p\n", &s[0]);
    printf("%p\n", &s[1]);
    printf("%p\n", &s[2]);
    printf("%p\n", &s[3]);
    printf("%p\n", &s[4]);
}
