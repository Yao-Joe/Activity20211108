/*
Linear search
/*

# include <stdio.h>
# include <string.h>

const int TOTAL = 7;

int main(void)
{
    int number[] = {1, 2, 3, 4, 5, 6, 7};
    for (int i = 0; i <= TOTAL; i++)
    {
        if (number[i] == 7 )
        {
            printf("Found\n");
            return 0;
        }
    }
    printf("NOT FUOND\n");
    return 1;
}
